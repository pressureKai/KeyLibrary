<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StateView">
        <attr name="emptyResource" format="reference" />
        <attr name="retryResource" format="reference" />
        <attr name="loadingResource" format="reference" />
    </declare-styleable>


    <declare-styleable name="Toolbar">
        <attr name="back_icon" format="integer"/>
        <attr name="left_title" format="string"/>
        <attr name="center_title" format="string"/>
        <attr name="left_title_show" format="boolean"/>
        <attr name="center_title_show" format="boolean"/>
        <attr name="right_view_show" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="EditTextView">
        <attr name="edit_hint" format="string" />
        <attr name="edit_clickable" format="boolean"/>
        <attr name="edit_hint_size" format="integer"/>
    </declare-styleable>
</resources>
